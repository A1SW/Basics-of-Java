import java.util.*;

public class solution{
 
  public static void main(String[] args) {
  
    Scanner sc = new Scanner(System.in);
   int units = sc.nextInt();
   int cost = units * 100;
  int discount = cost * 10/100;
  
  int totalcost;
  if(cost>1000)
  {
     totalcost = cost - discount;
     System.out.println(totalcost);
  }
  
  }
  }
